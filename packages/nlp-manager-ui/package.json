{
  "name": "@nlp-manager/ui",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "main": "dist/dist.js",
  "types": "src/dist.ts",
  "dependencies": {
    "@nlp-manager/shared": "*",
    "@blueprintjs/core": "^4.9.4",
    "@blueprintjs/datetime": "^4.3.10",
    "@blueprintjs/icons": "^4.4.2",
    "@blueprintjs/select": "^4.6.6",
    "@blueprintjs/table": "^4.6.5",
    "@heroicons/react": "^1.0.6",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "config": "^3.3.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "socket.io-client": "^4.5.1"
  },
  "scripts": {
    "start": "SUPPRESS_NO_CONFIG_WARNING=true craco start",
    "electron": "SUPPRESS_NO_CONFIG_WARNING=true BROWSER=none API_HOST=localhost API_PORT=8080 PORT=8082 craco start",
    "dist": "rm -rf ./dist && rm *.tsbuildinfo || true && mkdir dist && tsc --project tsconfig.dist.json",
    "build": "craco build",
    "build:electron": "PUBLIC_URL=./ craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "dev:electron": "yarn run electron"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-interactions": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/node-logger": "^6.4.20",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.20",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.4",
    "craco-interpolate-html-plugin": "^0.0.6",
    "http": "npm:stream-http",
    "postcss": "^8.4.12",
    "sass": "^1.50.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.4",
    "webpack": "^5.72.0"
  }
}
